<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
			  version="2.2">


	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<el-resolver>at.activesolution.samples.web.view.tomcat.EmptyToNullStringELResolver</el-resolver>
		<message-bundle>JsfValidation</message-bundle>
		<locale-config>
			<default-locale>de-AT</default-locale>
			<supported-locale>de-AT</supported-locale>
			<supported-locale>de-DE</supported-locale>
			<supported-locale>de</supported-locale>
		</locale-config>
	</application>


</faces-config>
